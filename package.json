{
  "name": "bevy-remote-devtools",
  "version": "0.1.0",
  "private": true,
  "description": "A remote developer tool for bevy applications",
  "keywords": [],
  "license": "MIT",
  "author": "Rene Eichhorn",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build:fe": "webpack --production",
    "build:tauri": "tauri build",
    "build": "npm run build:fe && npm run build:tauri",
    "dev": "concurrently -p \"[{name}]\" -n \"front-end,tauri\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run dev:fe\" \"npm run dev:tauri\"",
    "dev:fe": "webpack-dev-server --watch",
    "dev:tauri": "cross-env RUST_DEBUG=1 NODE_TLS_REJECT_UNAUTHORIZED=0 tauri dev"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.2.3",
    "@react-spring/web": "^9.3.1",
    "@tauri-apps/api": "^1.0.0-beta.8",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.35",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "glob": "^7.1.6",
    "history": "5",
    "html-webpack-plugin": "^4.3.0",
    "inline-source-cli": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "purgecss-webpack-plugin": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "6",
    "react-spring": "^9.3.2",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "three": "^0.135.0",
    "trim-newlines": "^3.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.2"
  }
}
